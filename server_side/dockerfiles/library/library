server {
                server_name   "library.web.dokr";
                listen 80;
                location / {
                        proxy_pass        http://127.0.0.1:4321;
                        proxy_set_header  X-Real-IP  $remote_addr;
                        proxy_set_header  Host $host;
                        #proxy_redirect http:// https://;
                        client_max_body_size 20m;
                }


}
